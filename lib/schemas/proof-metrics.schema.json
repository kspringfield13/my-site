{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://kylespringfield.dev/schemas/proof-metrics.schema.json",
  "type": "object",
  "required": ["timelineHighlights"],
  "properties": {
    "timelineHighlights": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["year", "label"],
        "properties": {
          "year": { "type": "integer" },
          "label": { "type": "string" }
        }
      }
    }
  }
}
